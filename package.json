{
  "name": "poloniex-watch",
  "productName": "Poloniex Watch",
  "version": "0.1.0",
  "description": "Poloniex ticker on your Menu Bar",
  "main": "main.js",
  "scripts": {
    "dev-server": "webpack-dev-server --hot --inline --config webpack.config.$NODE_ENV.js",
    "build": "webpack --config webpack.config.$NODE_ENV.js --progress --profile --colors",
    "start": "electron .",
    "package": "electron-packager",
    "release": "npm run build && npm run package",
    "development": "cross-env NODE_ENV=development concurrently --kill-others \"npm run dev-server\" \"npm run start\"",
    "production": "cross-env NODE_ENV=production npm run build && npm run start"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "dependencies": {
    "core-decorators": "^0.14.0",
    "d3": "^4.2.2",
    "electron": "^1.4.5",
    "electron-context-menu": "^0.6.0",
    "electron-debug": "^1.0.1",
    "electron-packager": "^8.2.0",
    "eslint-plugin-import": "^1.16.0",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.6",
    "mathjs": "^3.4.1",
    "menubar": "^5.1.0",
    "mobx": "^2.4.3",
    "mobx-react": "^3.5.5",
    "mobx-react-devtools": "^4.2.4",
    "moment": "^2.14.1",
    "node-sass": "^3.8.0",
    "plnx": "0.0.10",
    "promise-rate-limit": "0.0.1",
    "promisify-node": "^0.4.0",
    "react": "^15.3.1",
    "react-desktop": "^0.2.12",
    "react-dom": "^15.3.1",
    "react-stockcharts": "^0.5.0",
    "source-map-support": "^0.4.2",
    "thenby": "^1.2.1"
  },
